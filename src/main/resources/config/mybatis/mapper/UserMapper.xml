<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eastrobot.kbs.template.dao.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.eastrobot.kbs.template.model.entity.User">
        <id column="ID" property="id"/>
        <result column="USERNAME" property="username"/>
        <result column="PASSWORD" property="password"/>
        <result column="USER_INFO_ID" property="userInfoId"/>
        <result column="DELFLAG" property="delFlag"/>
        <result column="MAIN_STATION_ID" property="mainStationId"/>
        <result column="VISIT" property="visit"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, USERNAME, PASSWORD, USER_INFO_ID, DELFLAG, MAIN_STATION_ID, VISIT
    </sql>

</mapper>
